{
    "description": "Allows you to set the contents of a file input without uploading a file",
    "manifest_version": 3,
    "name": "noDiskUpload",
    "version": "0.1",
    "permissions": ["scripting", "activeTab"],

    "action": {
        "default_icon": "/icon.png",
        "default_popup": "index.html"
    },

    "icons": {
        "100":"icon.png"
    }
}